{
    "name": "chate-server",
    "version": "1.0.2",
    "description": "chate server build with nodejs express and mongodb",
    "main": "dist/server.js",
    "scripts": {
        "start": "npm run build && node dist/server.js",
        "build": "rimraf ./dist && tsc",
        "watch-ts": "tsc -w",
        "watch-node": "env-cmd -f ./env/dev.env nodemon dist/server.js",
        "dev": "concurrently \"yarn run watch-ts\" \"yarn run watch-node\""
    },
    "keywords": [
        "chate"
    ],
    "author": {
        "name": "Faysal Ahmed",
        "url": "https://www.github.com/faysal146"
    },
    "repository": {
        "url": "git@github.com:faysal146/chatE-react.git"
    },
    "license": "ISC",
    "_moduleAliases": {
        "@": "dist/"
    },
    "dependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-validator": "^3.0.0",
        "@types/http-errors": "^1.6.3",
        "@types/jimp": "^0.2.28",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.154",
        "@types/mongoose": "^5.7.21",
        "@types/morgan": "^1.9.0",
        "@types/multer": "^1.4.3",
        "@types/node": "^14.0.9",
        "@types/socket.io": "^2.1.8",
        "@types/validator": "^13.0.0",
        "async": "^3.2.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "env-cmd": "^10.1.0",
        "express": "^4.17.1",
        "express-validator": "^6.4.0",
        "http-errors": "^1.7.3",
        "jimp": "^0.13.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.19",
        "module-alias": "^2.2.2",
        "moment": "^2.26.0",
        "mongoose": "^5.9.4",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "passport-local": "^1.0.0",
        "slugify": "^1.4.0",
        "socket.io": "2.2.0",
        "validator": "^12.2.0"
    },
    "devDependencies": {
        "concurrently": "^5.2.0",
        "nodemon": "^2.0.2",
